<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="354.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.javafxapplication1.FileManagementController">
   <children>
      <Button fx:id="createfileBtn" layoutX="418.0" layoutY="254.0" mnemonicParsing="false" onAction="#createFiles" text="Create new file" />
      <Button fx:id="backBtn" layoutX="58.0" layoutY="302.0" mnemonicParsing="false" onAction="#switchToSecondary" text="Back" />
      <TextField fx:id="fileNameField" layoutX="52.0" layoutY="46.0" promptText="File Name" />
      <TextField fx:id="fileContentField" layoutX="52.0" layoutY="99.0" prefHeight="131.0" prefWidth="487.0" promptText="Text Contents" />
      <Button fx:id="saveContentsBtn" layoutX="52.0" layoutY="254.0" mnemonicParsing="false" onAction="#saveFileContentsTemp" text="Save contents to file" />
   </children>
</AnchorPane>
